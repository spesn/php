{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0",
            "excludeFiles": "{img/**}",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(\\w*)\\.(?:jpg|jpeg|png|gif|bmp|webp)",
            "dest": "/api/index.php?id=$1"
        }
    ]
}